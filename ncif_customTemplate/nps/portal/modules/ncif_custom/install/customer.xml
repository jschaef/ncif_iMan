<install>

	<module>
		<id>ncif_custom</id>
		<version>2.2.@TIMESTAMP@</version>
		<required-version>2.7.0</required-version>
		<expired-version>2.9.0</expired-version>
		<resource-properties-file>com.novell.ncif_custom.resources</resource-properties-file>
		<display-name-key>module.ncif_custom</display-name-key>
		<description-key>module.description</description-key>
	</module>

	<category>
		<id>ncif_category</id>
		<resource-properties-file>com.novell.ncif_custom.resources</resource-properties-file>
		<display-name-key>category.ncif_custom</display-name-key>
		<description-key>category.ncif_description</description-key>
	</category>

	<role>
		<id>ncif_custom_specific</id>
		<version>1.2</version>
		<required-version>2.7.0</required-version>
		<resource-properties-file>com.novell.ncif_custom.resources</resource-properties-file>
		<display-name-key>role.ncif_custom_specific</display-name-key>
		<order>500</order>
		<role-category-assignment>ncif_category</role-category-assignment>
	</role>

	<book>
		<id>ncif_custom_book_Linux_Device_Modification</id>
		<required-version>2.7.7</required-version>
		<resource-properties-file>com.novell.ncif_custom.resources</resource-properties-file>
		<display-name-key>book.ncif_custom_book_Linux_Device_Modification</display-name-key>
		<object-type-name>ncifDevice</object-type-name>
		<object-type-name>@CUSTOMER.ATTR.PREFIX.NCIF@Device</object-type-name>
		<class-name>com.novell.emframe.dev.DirPropertyBook</class-name>
		<role-assignment>ncif_custom_specific</role-assignment>
		<page-assignment>ncif_custom.ncif_custom_Linux_Device_Properties_General</page-assignment>
      <url-param>
         <param-key>mooEnabled</param-key>
         <param-value>false</param-value>
      </url-param>
	</book>

	<task>
		<id>ncif_custom_task_Linux_Device_Creation</id>
		<version>1.0</version>
		<required-version>1.0</required-version>
		<merge-template>ncif_custom.ncif_custom_Linux_Device_Creation</merge-template>
		<error-template>dev.GenErr</error-template>
		<resource-properties-file>com.novell.ncif_custom.resources</resource-properties-file>
		<display-name-key>task.ncif_custom_task_Linux_Device_Creation</display-name-key>
		<order>500</order>
		<url-param>
			<param-key>TC.multipleObjectEnabled</param-key>
			<param-value>true</param-value>
		</url-param>
		<url-param>
			<param-key>TC.advancedSelectionEnabled</param-key>
			<param-value>true</param-value>
		</url-param>
		<url-param>
			<param-key>eDirSearch</param-key>
			<param-value>Computer</param-value>
		</url-param>
		<url-param>
			<param-key>eDirCommand</param-key>
			<param-value>preCreate</param-value>
		</url-param>
		<defaultDisplayName>Linux Device Creation</defaultDisplayName>
		<taskType>snapinTask</taskType>
		<object-type-name>ncifConfiguration</object-type-name>
		<module>ncif_custom</module>
		<last-updated-by-imanager-version>2.7.7</last-updated-by-imanager-version>
		<menu-type>create</menu-type>
		<class-name>com.novell.emframe.dev.eDirAccessService</class-name>
		<role-assignment>ncif_custom_specific</role-assignment>
		<rights-assignment>
			<attribute-name>ncifIPAddresses</attribute-name>
			<privilege>Supervisor</privilege>
		</rights-assignment>
		<rights-assignment>
			<attribute-name>CN</attribute-name>
			<privilege>Supervisor</privilege>
		</rights-assignment>
		<rights-assignment>
			<attribute-name>ncifMACAddresses</attribute-name>
			<privilege>Supervisor</privilege>
		</rights-assignment>
		<rights-assignment>
			<attribute-name>Object Class</attribute-name>
			<privilege>Supervisor</privilege>
		</rights-assignment>
		<rights-assignment>
			<attribute-name>[Entry Rights]</attribute-name>
			<privilege>Browse</privilege>
			<privilege>Create</privilege>
		</rights-assignment>
	</task>
	
	<task>
		<id>ncif_custom_task_Linux_Device_Deletion</id>
		<version>2.0.0</version>
		<required-version>2.0.0</required-version>
		<merge-template>fw.DelObject</merge-template>
		<error-template>dev.GenFatal</error-template>
		<resource-properties-file>com.novell.ncif_custom.resources</resource-properties-file>
		<display-name-key>task.ncif_custom_task_Linux_Device_Deletion</display-name-key>
		<url-param>
			<param-key>objectTypeName</param-key>
			<param-value>ncifDevice</param-value>
		</url-param>
		<url-param>
			<param-key>TC.multipleObjectEnabled</param-key>
			<param-value>true</param-value>
		</url-param>
		<url-param>
			<param-key>TC.advancedSelectionEnabled</param-key>
			<param-value>true</param-value>
		</url-param>
		<taskType>snapinTask</taskType>
<!--
		<object-type-name>ncifDevice</object-type-name>
		<object-type-name>@CUSTOMER.ATTR.PREFIX.NCIF@Device</object-type-name>
-->
		<module>ncif_custom</module>
		<last-updated-by-imanager-version>2.7.7</last-updated-by-imanager-version>
		<class-name>java:com.novell.emframe.fw.DeleteObjectTask</class-name>
		<role-assignment>ncif_custom_specific</role-assignment>
		<rights-assignment>
			<attribute-name>[Entry Rights]</attribute-name>
			<privilege>Browse</privilege>
			<privilege>Delete</privilege>
		</rights-assignment>
	</task>

	<task>
		<id>ncif_custom_User_Settings</id>
		<version>2.0</version>
		<required-version>2.0</required-version>
		<class-name>com.novell.emframe.dev.eDirAccessService</class-name>
		<merge-template>ncif_custom.ncif_custom_User_Settings</merge-template>
		<error-template>dev.GenErr</error-template>
		<resource-properties-file>com.novell.ncif_custom.resources</resource-properties-file>
		<display-name-key>task.ncif_custom_User_Settings</display-name-key>
		<order>500</order>
		<description-key>task.ncif_custom_User_Settings</description-key>
		<object-type-name>User</object-type-name>
		<taskType>snapinTask</taskType>
		<module>ncif_custom</module>
		<role-assignment>User Management</role-assignment>
		<url-param>
			<param-key>TC.multipleObjectEnabled</param-key>
			<param-value>true</param-value>
		</url-param>
		<url-param>
			<param-key>TC.advancedSelectionEnabled</param-key>
			<param-value>true</param-value>
		</url-param>
		<url-param>
			<param-key>eDirSearch</param-key>
			<param-value>User</param-value>
		</url-param>
		<url-param>
			<param-key>defaultDisplayName</param-key>
			<param-value>ncif_custom_User_Settings</param-value>
		</url-param>
		<rights-assignment>
			<attribute-name>@CUSTOMER.ATTR.PREFIX.ATTR@Location</attribute-name>
			<privilege>Supervisor</privilege>
		</rights-assignment>
		<rights-assignment>
			<attribute-name>@CUSTOMER.ATTR.PREFIX.ATTR@InternetAccess</attribute-name>
			<privilege>Supervisor</privilege>
		</rights-assignment>
		<rights-assignment>
			<attribute-name>@CUSTOMER.ATTR.PREFIX.ATTR@Country</attribute-name>
			<privilege>Supervisor</privilege>
		</rights-assignment>
		<rights-assignment>
			<attribute-name>@CUSTOMER.ATTR.PREFIX.ATTR@Company</attribute-name>
			<privilege>Supervisor</privilege>
		</rights-assignment>
		<rights-assignment>
			<attribute-name>dialupAccess</attribute-name>
			<privilege>Supervisor</privilege>
		</rights-assignment>
		<rights-assignment>
			<attribute-name>Object Class</attribute-name>
			<privilege>Supervisor</privilege>
		</rights-assignment>
	</task>



</install>
